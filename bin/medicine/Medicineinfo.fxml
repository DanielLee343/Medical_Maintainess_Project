<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="medicine.MedicineInfoController">
   <center>
      <AnchorPane prefHeight="500.0" prefWidth="700.0" BorderPane.alignment="CENTER">
         <children>
            <Text layoutX="45.0" layoutY="78.0" strokeType="OUTSIDE" strokeWidth="0.0" text="药品信息维护：">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <Text layoutX="242.0" layoutY="184.0" strokeType="OUTSIDE" strokeWidth="0.0" text="请输入药品名称：">
               <font>
                  <Font size="27.0" />
               </font>
            </Text>
            <TextField id="name" fx:id="textfield1" layoutX="199.0" layoutY="235.0" prefHeight="30.0" prefWidth="303.0" />
            <Button layoutX="75.0" layoutY="402.0" mnemonicParsing="false" onAction="#zengjia" text="增加药品">
               <font>
                  <Font size="23.0" />
               </font>
            </Button>
            <Button layoutX="311.0" layoutY="325.0" mnemonicParsing="false" onAction="#queren" prefHeight="30.0" prefWidth="78.0" text="确认">
               <font>
                  <Font size="19.0" />
               </font>
            </Button>
            <Button layoutX="482.0" layoutY="402.0" mnemonicParsing="false" onAction="#fanhui" prefHeight="46.0" prefWidth="124.0" text="返回">
               <font>
                  <Font size="23.0" />
               </font>
            </Button>
            <Label fx:id="warning" layoutX="283.0" layoutY="276.0" opacity="0.0" text="药品名称不能为空！" />
         </children>
      </AnchorPane>
   </center>
</BorderPane>
